{
  "name": "@wasedatime/root-config",
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "pnpm run tailwind:build && vite --port 9000",
    "build": "vite build",
    "build-dev": "vite build",
    "preview": "vite preview --port 9000",
    "preview-dev": "vite preview --port 9000",
    "tsc": "tsc",
    "fix": "pnpm run format && pnpm run lint:fix",
    "lint": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "local": "concurrently --kill-others \"pnpm start\" \"cd ../syllabus && pnpm start\" \"cd ../campus && pnpm start\" \"cd ../career && pnpm start\" \"cd ../blog && pnpm start\"",
    "tailwind:build": "pnpx tailwindcss build -i ./src/styles/styles.css -o ./src/styles/tailwind-build-output.css"
  },
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@babel/eslint-parser": "7.18.2",
    "@babel/plugin-syntax-flow": "7.17.12",
