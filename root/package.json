  "scripts": {
    "start": "npm run watch:css && webpack serve --port 3000 --env isLocal",
    "lint": "eslint src --ext js,ts,tsx",
    "test": "cross-env BABEL_ENV=test jest --passWithNoTests",
    "format": "prettier --write \"./**\"",
    "build": "webpack --mode=production",
    "build-dev": "webpack --mode=production --env isDev",
    "local": "npm run watch:css & concurrently --kill-others \"npm start\" \"cd ../syllabus && npm start\" \"cd ../campus && npm start\" \"cd ../career && npm start\"",
