    "@vitejs/plugin-react": "1.3.2",
    "autoprefixer": "10.4.20",
    "babel-plugin-styled-components": "2.0.7",
    "concurrently": "7.2.1",
    "cssnano": "5.1.11",
    "dotenv": "16.0.1",
    "esbuild": "^0.18.6",
    "eslint": "8.17.0",
    "eslint-config-custom": "workspace:*",
    "postcss": "8.4.31",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "prop-types": "15.8.1",
    "react-is": "18.1.0",
    "react-native": "0.68.2",
